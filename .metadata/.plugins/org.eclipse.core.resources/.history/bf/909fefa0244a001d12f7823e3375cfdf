package conditions;

public class Operation {

	public static void main(String[] args) {
		int x=10;
		int y=x--;
		int z=x;
		System.out.println(y);
		System.out.println(z);
		System.out.println(x++); //outputs x=10 then increments
		System.out.println(x); //outputs x=11
		x++;
		System.out.println(x); //outputs x=12
		int m=10;
		int n=20;
		if(m != n)
			System.out.println("they are not equal");
		
		

	}

}
